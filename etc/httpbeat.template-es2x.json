{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": {
          "enabled": false
        }
      },
      "_meta": {
        "version": "2.0.0"
      },
      "dynamic_templates": [
        {
          "strings_as_keyword": {
            "mapping": {
              "ignore_above": 1024,
                "index": "not_analyzed",
                "type": "string"
            },
          "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "request": {
          "properties": {
            "headers": {
              "properties": {}
            }
          }
        },
        "response": {
          "properties": {
            "headers": {
              "properties": {}
            }
          }
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "template": "httpbeat-*"
}
